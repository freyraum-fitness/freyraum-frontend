{
  "name": "freyraum-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.config.js --mode development",
    "build": "webpack --config ./webpack.config.js",
    "build_tst": "webpack --config webpack.tst.config.js",
    "build_int": "webpack --config webpack.int.config.js",
    "test": "jest --coverage --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shauschi/freyraum-frontend.git"
  },
  "keywords": [],
  "author": "Stefan Hauschildt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shauschi/freyraum-frontend/issues"
  },
  "homepage": "https://github.com/shauschi/freyraum-frontend#readme",
  "jest": {
    "automock": false,
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "dependencies": {
    "@date-io/moment": "0.0.2",
    "@devexpress/dx-react-core": "1.6.1",
    "@devexpress/dx-react-grid": "1.6.1",
    "@devexpress/dx-react-grid-material-ui": "1.6.1",
    "@material-ui/core": "3.4.0",
    "@material-ui/icons": "3.0.1",
    "chart.js": "1.1.1",
    "express": "4.16.4",
    "fuse.js": "3.3.0",
    "material-ui-pickers": "2.0.4",
    "mdi-material-ui": "5.4.0",
    "moment": "2.22.2",
    "path": "0.12.7",
    "pulltorefreshjs": "0.1.14",
    "ramda": "0.26.0",
    "react": "16.6.1",
    "react-avatar-editor": "11.0.4",
    "react-chartjs": "1.2.0",
    "react-cookie": "3.0.4",
    "react-dom": "16.6.1",
    "react-hammerjs": "1.0.1",
    "react-lazyload": "^2.3.0",
    "react-loadable": "5.5.0",
    "react-player": "1.7.1",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-slick": "0.23.2",
    "react-swipeable-views": "0.13.0",
    "react-swipeable-views-utils": "0.13.0",
    "react-transition-group": "2.5.0",
    "redux": "4.0.1",
    "redux-actions": "2.6.4",
    "redux-thunk": "2.3.0",
    "slick-carousel": "1.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.1.5",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.5",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.1.2",
    "autoprefixer": "9.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.0-beta.6",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "0.28.9",
    "enzyme": "3.7.0",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "3.2.0",
    "jest": "21.2.1",
    "jest-enzyme": "4.2.0",
    "jest-fetch-mock": "1.6.2",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "react-hot-loader": "4.3.12",
    "redux-logger": "3.0.6",
    "style-loader": "0.19.1",
    "uglifyjs-webpack-plugin": "v1.0.0-beta.1",
    "url-loader": "0.6.2",
    "webpack": "4.25.1",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  }
}
